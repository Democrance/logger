{
    "name": "@democrance/logger",
    "version": "0.2.0",
    "description": "Beautiful alternative for console logging",
    "keywords": [
        "console",
        "logger",
        "log"
    ],
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "repository": "daniil4udo/logger",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && tsup",
        "lint": "eslint --fix --ext .ts,.js .",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "test": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^0.9.0",
        "@types/node": "^18.17.1",
        "@vitest/coverage-c8": "^0.33.0",
        "@vue/tsconfig": "^0.4.0",
        "bumpp": "^9.1.1",
        "eslint": "^8.45.0",
        "tsup": "^7.1.0",
        "typescript": "^5.1.6",
        "vitest": "^0.33.0"
    },
    "packageManager": "pnpm@8.6.5",
    "engines": {
        "node": ">=14"
    },
    "pnpm": {
        "overrides": {
            "rollup": "^3.0.0"
        }
    }
}
